#include <cstdio>
#include <iostream>
#include <cmath>
#include <algorithm>
#include <string.h>
typedef long long ll;
using namespace std;

int ar[1000000];

int main() {
	int A, N;
	scanf("%d%d", &A, &N);
	
	if (N == 0) {
		printf("0\n");
		return 0;
	}
		
	ll left = 0, v;
	int cnt = 0;
	for (int i = 0; i < N || left > 0; i++) {
		v = (N - i) * A + left;
		ar[i] = v % 10;
		left = v / 10;
		cnt++;
		//printf("%lld %lld %d   ", v, left, ar[i]);
	}
//	printf("\n");
	for (int i = 0; i < cnt; i++) {
		printf("%d", ar[cnt - i - 1]);
	}
	printf("\n");
	return 0;
}
